<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.grupo25.tp_obj2_hibernate.model.entities.Etiqueta" table="etiquetas">
        <id column="id" name="id">
            <generator class="identity" />
        </id>
        <property name="nombre" column="nombre" type="string" />
        <property name="createAt" column="create_at_etiqueta" type="timestamp" />
        <property name="updateAt" column="update_at_etiqueta" type="timestamp" />
        
        <!-- RelaciÃ³n muchos a muchos con Ticket -->
        <set name="tickets" table="ticket_etiqueta">
            <key column="etiqueta_id" />
            <many-to-many column="ticket_id" class="com.grupo25.tp_obj2_hibernate.model.entities.Ticket" />
        </set>
    </class>
</hibernate-mapping> 